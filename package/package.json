{
  "name": "@crowdstrike/graphics-core",
  "version": "1.2.0",
  "description": "WebGL primitives and utility functions that power CrowdStrike's graphs.",
  "keywords": [
    "threejs",
    "WebGL"
  ],
  "repository": {
    "type": "git",
    "url": "git@github.com:CrowdStrike/graphics.git",
    "directory": "package"
  },
  "license": "MIT",
  "author": "CrowdStrike UX Team",
  "exports": {
    ".": "./dist/index.js",
    "./test-support": "./dist/test-support/index.js"
  },
  "typesVersions": {
    "*": {
      "test-support": [
        "./dist/test-support/index.d.ts"
      ],
      "*": [
        "./dist/*"
      ]
    }
  },
  "files": [
    "dist",
    "src"
  ],
  "scripts": {
    "build": "pnpm build:js",
    "build:docs": "cp ../README.md ./README.md",
    "build:js": "rollup -c ./config/rollup.config.mjs",
    "lint": "concurrently 'pnpm:lint:js'",
    "lint:fix": "concurrently 'pnpm:lint:js:fix'",
    "lint:js": "eslint .",
    "lint:js:fix": "eslint . --fix",
    "prepack": "pnpm build",
    "prepublishOnly": "pnpm build",
    "start": "pnpm watch:js",
    "watch": "concurrently 'pnpm:watch:*'",
    "watch:js": "pnpm build:js --watch"
  },
  "dependencies": {
    "gsap": "3.11.5",
    "three": "0.154.0"
  },
  "devDependencies": {
    "@babel/core": "7.22.10",
    "@babel/eslint-parser": "^7.19.1",
    "@babel/plugin-proposal-class-properties": "7.18.6",
    "@babel/plugin-proposal-decorators": "7.21.0",
    "@babel/plugin-proposal-private-methods": "7.18.6",
    "@babel/plugin-syntax-decorators": "7.21.0",
    "@babel/plugin-syntax-dynamic-import": "7.8.3",
    "@babel/plugin-transform-runtime": "7.21.4",
    "@babel/plugin-transform-typescript": "7.21.3",
    "@babel/preset-env": "7.16.5",
    "@babel/preset-typescript": "7.21.5",
    "@babel/runtime": "7.21.5",
    "@embroider/addon-dev": "3.0.0",
    "@glimmer/component": "1.1.2",
    "@nullvoxpopuli/eslint-configs": "^3.2.2",
    "@types/three": "0.152.1",
    "@typescript-eslint/eslint-plugin": "^5.62.0",
    "@typescript-eslint/parser": "^5.62.0",
    "concurrently": "7.6.0",
    "eslint": "8.33.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-decorator-position": "^5.0.0",
    "eslint-plugin-import": "2.26.0",
    "eslint-plugin-json": "3.1.0",
    "eslint-plugin-n": "^16.3.0",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-simple-import-sort": "8.0.0",
    "prettier": "^3.0.0",
    "rollup": "3.27.0",
    "rollup-plugin-copy": "^3.5.0",
    "rollup-plugin-ts": "3.4.5",
    "typescript": "5.0.2",
    "webpack": "5.76.2"
  },
  "peerDependencies": {
    "gsap": ">=3.11.5",
    "three": ">=0.154.0"
  },
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org"
  },
  "packageManager": "pnpm@7.3.0",
  "volta": {
    "extends": "../package.json"
  }
}
